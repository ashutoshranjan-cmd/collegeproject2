    <!-- Sidebar sidebar Item -->
    <div class="xs-sidebar-group info-group">
        <div class="xs-overlay xs-bg-black" id="gallary-box">
            <img src="~/Content/gallaryImage/gallary-image1.png" id="view" />
        </div>
        <div class="xs-sidebar-widget">
