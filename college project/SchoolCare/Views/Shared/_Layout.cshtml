<body class="xis-dia-body">
    <div id="preloader"></div>
    <div class="up">
        <a href="#" class="scrollup text-center"><i class="fas fa-chevron-up"></i></a>
    </div>
    <!-- CHAT BAR BLOCK -->
    <div class="chat-bar-collapsible">
        <button id="chat-button" type="button" class="collapsible">
         
            <i id="chat-icon" style="color: #fff;" class="fa fa-fw fa-comments-o"></i>
        </button>

        <div class="chat_content">
            <div class="full-chat-block">

                <div class="outer-container">
                    <div class="chat-container">
                        Messages
                        <div id="chatbox">
                            <h5 id="chat-timestamp"></h5>
                            <p id="botStarterMessage" class="botText"><span>Loading...</span></p>
                        </div>


                        <div class="chat-bar-input-block">
                            <div id="userInput">
                                <input id="textInput" class="input-box" type="text" name="msg"
                                       placeholder="Press enter to send a message">
                                <p></p>
                            </div>

                            <div class="chat-bar-icons">

                                <i id="chat-icon" style="color: #333;" class="fa fa-fw fa-send"
                                   onclick="sendButton()"></i>
                            </div>
                        </div>

                        <div id="chat-bar-bottom">
                            <p></p>
                        </div>

                    </div>
                </div>

            </div>
        </div>

    </div>

    <!-- Start of header section
    ============================================= -->
    <header id="xis-dia-header" class="xis-dia-header-section">
        <div class="container">
            <div class="xis-dia-header-~/Content">
                <div class="xisma-header-main-menu d-flex align-items-center justify-content-between">
                    <div class="site-logo">
                        <a href="/">
                            <img src="~/Content/assets/C_Image/school-care-p.png?width=120" alt="SchoolCare" title="SchoolCare" />
                        </a>
                    </div>
                    <div class="xis-dia-main-navigation-area">
                        <nav class="xis-dia-main-navigation clearfix ul-li">
                            <ul id="xis-dia-main-nav" class="nav navbar-nav clearfix">
                                <li><a href="/">Home</a></li>
                                <li><a href="/#xis-dia-about">About Us</a></li>
                                <li class="dropdown">
                                    <a href="#">Services</a>
                                    <ul class="dropdown-menu clearfix">

                                        <li><a href="/App">Mobile App</a></li>
                                        @*<li><a href="/Management">School Management Software</a></li>*@
                                        <li><a href="/ELearning">E-learning Software</a></li>
                                        @*<li><a href="/Faculties">Website designing</a></li>*@
                                        <li><a href="/WhatsappBot">Whatsapp Group</a></li>
                                        <li><a href="/Tracking">Advanced vehicle tracking</a></li>
                                    </ul>
                                </li>
                                <li class="dropdown">
                                    <a href="#">More Info</a>
                                    <ul class="dropdown-menu clearfix">
                                        <li><a href="/#xis-dia-testimonial">Testimonial</a></li>
                                        <li><a href="/#xis-dia-team">Team</a></li>
                                        <li><a href="/Faculties">Faculties</a></li>
                                        @*<li><a href="/Faculties">Why School care ?</a></li>
                                        <li><a href="/Dashboard">Dashboard</a></li>*@
                                        
                                        
                                    </ul>
                                </li>
                                <li><a href="/Contact">Contact Us</a></li>
                                <li><a href="/Blog">Blog</a></li>
                                <li id="login-link-pos" ><a id="login-li-a" href="/Login">Login</a></li>
                               
                            </ul>
                        </nav>
                    </div>
                    <div class="xis-side-canvas-btn navSidebar-button  ">
                        <span class="gallary-icon"></span>
                        <span class="gallary-icon"></span>
                        <span class="gallary-icon"></span>
                    </div>
                </div>
                <div class="mobile_menu position-relative">
                    <div class="mobile_menu_button open_mobile_menu">
                        <i class="fas fa-bars"></i>
                    </div>
                    <div class="mobile_menu_wrap">
                        <div class="mobile_menu_overlay open_mobile_menu"></div>
                        <div class="mobile_menu_content">
                            <div class="mobile_menu_close open_mobile_menu">
                                <i class="fas fa-times"></i>
                            </div>
                            <div class="m-brand-logo">
                                <a href="/"><img src="~/Content/assets/C_Image/school-care-white.png?width=120" alt=""></a>
                            </div>
                            <nav class="mobile-main-navigation clearfix ul-li">
